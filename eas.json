{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_STAGE": "prod",
        "EXPO_PUBLIC_API_URL": "https://backend-teslo-shop-latest.onrender.com/api",
        "EXPO_PUBLIC_API_URL_IOS": "https://backend-teslo-shop-latest.onrender.com/api",
        "EXPO_PUBLIC_API_URL_ANDROID": "https://backend-teslo-shop-latest.onrender.com/api"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
